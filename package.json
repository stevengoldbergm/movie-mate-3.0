{
  "name": "movie-mate-3.0",
  "version": "0.1.0",
  "engines": {
    "node": "16.17.0"
  },
  "private": true,
  "dependencies": {
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "scripts": {
    "start": "node server/server.js",
